<Page x:Class="Hakaton.Pages.RegEnterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Hakaton.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="650"
      d:DesignWidth="800"
      Title="RegEnterPage">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>

        <Frame x:Name="frmLeftPart" NavigationUIVisibility="Hidden"/>

        <Border Width="2" Background="AliceBlue" HorizontalAlignment="Right" Margin="0 20 0 20" Panel.ZIndex="1"/>

        <Frame x:Name="frmRightPart" NavigationUIVisibility="Hidden" Grid.Column="1" JournalOwnership="OwnsJournal"/>
    </Grid>
</Page>
